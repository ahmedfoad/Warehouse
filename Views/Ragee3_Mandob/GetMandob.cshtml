<div class="row" style="height:150px;">
    <div class="col-md-12">
        <!-- BEGIN Portlet PORTLET-->
        <div class="portlet light">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-paper-plane font-yellow-casablanca" style="color: #5c9acf !important;"></i>
                    من فضلك اختار مندوب المبيعات
                </div>
                <div class="actions">
                    <a class="btn red btn-sm fancymodal-close" id='closebox' ng-click="closeUiModal_Mandob()" style="padding-left:15px;" href="#"> إغلاق </a>
                </div>
            </div>
            <div class='portlet-body'>
                <div class='row' style="margin: auto;">
                    <input id="Search" ng-keyup="SearchMandob()" class='form-control' placeholder='بحث باسم مندوب المبيعات' type='text' />
                </div>
                <div class='row' style="text-align:center;">
                    <div class='table-responsive' style="max-height: 300px;margin-top:10px;" id="ViewList">
                        <table class='table table-striped' style='width:90%;margin:auto'>
                            <thead>
                                <tr>
                                    <th> اسم مندوب المبيعات  </th>
                                    <th> رقم الجوال </th>
                                    <th> ملاحظات </th>
                                </tr>
                            </thead>
                            <tbody id="GetDealingTypeTable">
                                <tr ng-repeat="item in MandobList track by $index" ng-click="clicktrMandob(item)">
                                   
                                    <td>{{item.Name}}</td>
                                    <td>{{item.JawalNO}}</td>
                                    <td>{{item.Address}}</td>
                                </tr>
                            </tbody>
                        </table>

                    </div>

                </div>
            </div>
        </div>
        <!-- END Portlet PORTLET-->
    </div>
</div>




<style>
    .modal.in .modal-dialog {
        height: 150px;
        width: 1200px;
    }
</style>