<div class="row" style="height:200px;">
    <div class="col-md-12">
        <!-- BEGIN Portlet PORTLET-->
        <div class="portlet light">

            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-paper-plane font-yellow-casablanca" style="color: #5c9acf !important;"></i>
                    <a href="#" id="pageTitle">
                        إضافة  مندوب مبيعات
                    </a>
                </div>
                <div class="actions">
                    <a class="btn red btn-sm fancymodal-close" id='closebox' ng-click="closeUiModal()" style="padding-left:15px;" href="#"> إغلاق </a>
                </div>
            </div>
            <div class='portlet-body'>
                <div class="col-md-12" ng-show="AlertParameter">
                    <div class="m-heading-1 border-green m-bordered">
                        <span style="color:red"> {{ErrorName}} </span>
                        <button type="button" style="margin-top: 6px;" class="close" ng-click="ReomveAlert()"></button>
                    </div>
                </div>
                <div class='row' style="margin: auto;">
                    <label for="Name">اسم مندوب المبيعات</label>
                    <input ng-model="Mandob.Name" id="Name" class='form-control' placeholder='اسم مندوب المبيعات' type='text' />
                    <input id="ID" ng-model="Mandob.ID" type="hidden" class='form-control' />
                </div>
                <div class='row' style="margin: auto;">
                    <label for="Name">رقم الجوال</label>
                    <input ng-model="Mandob.JawalNO" id="Name" class='form-control' placeholder='اسم مندوب المبيعات' type='text' />
                </div>
               
                <div class='row' style="margin: auto;">
                    <label for="IP">ملاحظات</label>
                    <textarea rows="4" cols="50" ng-model="Mandob.Address" class='form-control' placeholder='ملاحظات'></textarea>
                </div>

            </div>
            <div class='row form-actions' style='text-align:center'>
                <button type='submit' class='btn green' ng-hide="SaveBtn" ng-click="SaveData(Mandob)">حفظ</button>
            </div>
        </div>
        <!-- END Portlet PORTLET-->


    </div>


</div>




