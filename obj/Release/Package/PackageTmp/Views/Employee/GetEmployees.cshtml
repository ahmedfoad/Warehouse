<div class="row"  style="height:150px;">
    <div class="col-md-12">
        <!-- BEGIN Portlet PORTLET-->
        <div class="portlet light">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-paper-plane font-yellow-casablanca" style="color: #5c9acf !important;"></i>
                    من فضلك اختار اسم الموظف
                </div>
                <div class="actions">
                    <a class="btn red btn-sm fancymodal-close" id='closebox' ng-click="closeUiModal()" style="padding-left:15px;" href="#"> إغلاق </a>
                </div>
            </div>
            <div class='portlet-body'>
                <div class='row' style="margin: auto;">
                    <input id="Search" ng-keyup="keyupevt()" class='form-control' placeholder='بحث باسم الموظف' type='text' />
                </div>
                <div class='row' style="text-align:center;">
                    <div class='table-responsive' style="max-height: 300px;margin-top:10px;" id="ViewList">
                        <table class='table table-striped' style='width:90%;margin:auto'>
                            <thead>
                                <tr>
                                    <th> اسم الموظف </th>
                                </tr>
                            </thead>
                            <tbody id="GetDealingTypeTable">
                                <tr ng-repeat="item in data track by $index" ng-click="clicktr_Employees(item)">
                                    <td style="width:70%">{{item.Name}}</td>
                                </tr>
                            </tbody>
                        </table>
                        <a href="#" @*ng-show="hideloadmore"*@ ng-click="LoadMore()"> المزيد </a>
                        @*<p style="color:red;" ng-show="otherData">لا توجد بيانات آخري</p>*@
                    </div>

                </div>
            </div>
        </div>
        <!-- END Portlet PORTLET-->
    </div>
</div>




